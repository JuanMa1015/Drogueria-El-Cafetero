@model Drogueria_Elcafetero.Models.suppliers_products

@{
    ViewData["Title"] = "Detalles de Productos de Proveedores";
    Layout = null;
}
<h1>Detalles de Productos de Proveedores</h1>
<link rel="stylesheet" href="~/css/detailsAdmin.css">
<div>
    <h4>Productos de Proveedores</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.id_product)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.id_product)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.id_supplier)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.id_supplier)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.price)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.price)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.agreement_date)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.agreement_date)
        </dd>
    </dl>
</div>
<div class="actions">
    <a class="btn" asp-action="Edit" asp-route-id="@Model?.id_supplier_product">Editar</a> |
    <a class="btn" asp-action="Index">Volver al Listado</a>
</div>